{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "My attempt of solving Advent of Code puzzles.",
  "private": true,
  "scripts": {
    "check": "tsc --noEmit",
    "format": "prettier --cache --ignore-path .gitignore --write .",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/advent-of-code.git"
  },
  "keywords": [
    "advent-of-code",
    "advent",
    "algorithms",
    "christmas",
    "problem-solving",
    "puzzle",
    "solutions"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/advent-of-code/issues"
  },
  "homepage": "https://github.com/marcobiedermann/advent-of-code#readme",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "lint-staged": {
    "**/*": "npm run format"
  }
}
